<table>
    <thead>
        <tr>
            <th>Full Name</th>
            <th>Last Name</th>
            <th>Mobile</th>
            <th>City</th>
        </tr>
    </thead>
    <tbody>
       {{#each list}}
       <tr>
       <td>{{this.fullname}}</td>
       <td>{{this.lastname}}</td>
       <td>{{this.mobile}}</td>
       <td>{{this.city}}</td>
       <td>
           <a href="/student/{{this._id}}" style="background-color: aqua;">Update</a>
       </td>
       <td><a href="/student/delete/{{this._id}}" style="background-color: red;">Delete</a></td>
       </tr>
       {{/each}}
    </tbody>
</table>